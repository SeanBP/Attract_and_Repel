   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Grid Initializing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_zero_grid(n):\n",
    "    return [[0] * n for _ in range(n)]\n",
    "\n",
    "def generate_random_grid(n, a, b):\n",
    "    grid = []\n",
    "    for _ in range(n):\n",
    "        row = []\n",
    "        for _ in range(n):\n",
    "            rand_num = random.randint(1, 100)\n",
    "            if rand_num <= a:  \n",
    "                row.append(1)\n",
    "            elif rand_num <= b: \n",
    "                row.append(-1)\n",
    "            else:\n",
    "                row.append(0)\n",
    "        grid.append(row)\n",
    "    return grid"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Rules"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_angle(grid, x, y, mult):\n",
    "    dx = [-1, 0, 1, -1, 1, -1, 0, 1]\n",
    "    dy = [-1, -1, -1, 0, 0, 1, 1, 1]\n",
    "\n",
    "    vector_sum_x = 0\n",
    "    vector_sum_y = 0\n",
    "\n",
    "    for i in range(8):\n",
    "        nx = (x + dx[i]) % len(grid)\n",
    "        ny = (y + dy[i]) % len(grid)\n",
    "        vectx = dx[i] * mult*abs(grid[nx][ny])\n",
    "        vecty = dy[i] * mult*abs(grid[nx][ny])\n",
    "            \n",
    "        vector_sum_x += vectx\n",
    "        vector_sum_y += vecty\n",
    "            \n",
    "    angle_rad = math.atan2(-vector_sum_y, vector_sum_x)\n",
    "\n",
    "    angle_deg = math.degrees(angle_rad)\n",
    "    \n",
    "    if vector_sum_x == 0 and vector_sum_y == 0:\n",
    "        return None\n",
    "    \n",
    "    if angle_deg < 0:\n",
    "        angle_deg += 360\n",
    "        \n",
    "    return angle_deg\n",
    "\n",
    "def get_neighbors_at_angle(x, y, grid_size, angle):\n",
    "    dx = [-1, 0, 1, -1, 1, -1, 0, 1]\n",
    "    dy = [-1, -1, -1, 0, 0, 1, 1, 1]\n",
    "\n",
    "    neighbors = []\n",
    "\n",
    "    for i in range(8):\n",
    "        nx = (x + dx[i]) % grid_size\n",
    "        ny = (y + dy[i]) % grid_size\n",
    "        neighbor_angle = math.atan2(dy[i], -dx[i]) * (180 / math.pi)\n",
    "        if neighbor_angle < 0:\n",
    "            neighbor_angle += 360\n",
    "        if abs((neighbor_angle - angle + 180) % 360 - 180) < 45:\n",
    "            neighbors.append((ny, nx))\n",
    "\n",
    "    return neighbors\n",
    "\n",
    "def add_coordinates(grid, coordinates):\n",
    "    for x, y in coordinates:\n",
    "        grid[y][x] = grid[y][x] + 1\n",
    "    return grid\n",
    "\n",
    "def subtract_coordinates(grid, coordinates):\n",
    "    for x, y in coordinates:\n",
    "        grid[y][x] = grid[y][x] - 1\n",
    "    return grid\n",
    "\n",
    "def adjust_grid_values(grid):\n",
    "    adjusted_grid = []\n",
    "    for row in grid:\n",
    "        adjusted_row = []\n",
    "        for value in row:\n",
    "            if value > 0:\n",
    "                adjusted_row.append(1)\n",
    "            elif value < 0:\n",
    "                adjusted_row.append(-1)\n",
    "            else:\n",
    "                adjusted_row.append(0)\n",
    "        adjusted_grid.append(adjusted_row)\n",
    "    return adjusted_grid\n",
    "\n",
    "def next_step(old_grid):\n",
    "    grid_size = len(old_grid)\n",
    "    new_grid = get_zero_grid(grid_size)\n",
    "    \n",
    "    for x in range(grid_size):\n",
    "        for y in range(grid_size):            \n",
    "            if old_grid[x][y] != 0:\n",
    "                angle = get_angle(old_grid, x, y, old_grid[x][y])\n",
    "                if angle is not None:\n",
    "                    neighbors = get_neighbors_at_angle(x, y, grid_size, angle)\n",
    "                    if old_grid[x][y] > 0:\n",
    "                        new_grid = add_coordinates(new_grid, neighbors)\n",
    "                    if old_grid[x][y] < 0:\n",
    "                        new_grid = subtract_coordinates(new_grid, neighbors)\n",
    "    new_grid = adjust_grid_values(new_grid)\n",
    "    return new_grid"
   ]
